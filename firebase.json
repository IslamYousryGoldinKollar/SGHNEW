{
  "hosting": {
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "studio",
          "region": "us-central1"
        }
      }
    ]
  },
  "firestore": {
    "rules": "src/firestore.rules"
  },
  "apphosting": {
    "backends": {
      "studio": {
        "source": ".next",
        "region": "us-central1"
      }
    }
  }
}